{
  "name": "@kiwilan/vite-plugin-laravel-steward",
  "version": "0.0.1",
  "description": "Vite plugin for laravel-steward.",
  "author": "Ewilan Rivi√®re <ewilan.riviere@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kiwilan/vite-plugin-laravel-steward",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiwilan/vite-plugin-laravel-steward"
  },
  "bugs": {
    "url": "https://github.com/kiwilan/vite-plugin-laravel-steward/issues"
  },
  "keywords": [
    "steward",
    "vite"
  ],
  "exports": {
    ".": {
      "require": "./dist/laravel-steward.umd.js",
      "import": "./dist/laravel-steward.es.js"
    }
  },
  "main": "./dist/laravel-steward.umd.js",
  "module": "./dist/laravel-steward.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean",
    "watch": "tsup --watch",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@tiptap/core": "2.0.0-beta.195",
    "@tiptap/extension-character-count": "2.0.0-beta.195",
    "@tiptap/extension-link": "2.0.0-beta.195",
    "@tiptap/extension-typography": "2.0.0-beta.195",
    "@tiptap/starter-kit": "2.0.0-beta.195",
    "@types/alpinejs": "^3.7.0",
    "@types/node": "^18.7.18",
    "alpinejs": "^3.10.3",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.23.1",
    "husky": "^8.0.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "vite": "^3.1.2"
  }
}