{
  "name": "@kiwilan/vite-plugin-laravel-steward",
  "version": "0.0.136",
  "description": "Vite plugin for laravel-steward.",
  "author": "Ewilan Rivi√®re <ewilan.riviere@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kiwilan/vite-plugin-laravel-steward",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiwilan/vite-plugin-laravel-steward.git"
  },
  "bugs": {
    "url": "https://github.com/kiwilan/vite-plugin-laravel-steward/issues"
  },
  "keywords": [
    "steward",
    "vite"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./css/tiptap.css": {
      "import": "./lib/css/tiptap.css",
      "require": "./lib/css/tiptap.css"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "directories": {
    "dist": "dist",
    "lib": "lib"
  },
  "scripts": {
    "build": "tsup --clean",
    "watch": "tsup --watch",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
