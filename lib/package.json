    "@tiptap/extension-typography": "2.0.0-beta.196",
    "@tiptap/starter-kit": "2.0.0-beta.196",
    "@types/alpinejs": "^3.7.0",
    "@types/node": "^18.7.20",
    "alpinejs": "^3.10.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  }
